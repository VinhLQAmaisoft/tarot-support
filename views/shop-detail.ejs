                                        <div class="discount-label green">
                                            <span>
                                                <%=product.discount*100%>%
                                            </span>
                                        </div>
                                        <a href="<%=product.featureImage[0]%>"
                                            class="yith_magnifier_zoom woocommerce-main-image" title="product-19">
                                            <img src="<%=product.featureImage[0]%>" alt="" />
                                        </a>
                                        <div class="thumbnails noslider">
